{
  "manifest_version": 2,
  "name": "jk-nav-ff",
  "version": "1.0",
  "description": "this does something",
  "homepage_url": "asdf",
  
  "icons": {
    "48": "icons/icon-48.png"
  },

  "permissions": [
    "activeTab"
  ],

  "content_scripts": [
    {
      "matches": ["*://*/"],
      "js": ["jquery-3.3.1.min.js", "js/contentScript.js"],
      "run_at": "document_start"
    }
  ],

  "background": {
    "scripts": ["jquery-3.3.1.min.js", "js/background-script.js"]  
  }

}
  
