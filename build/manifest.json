{
  "manifest_version": 2,
  "name": "jk-nav-ff",
  "version": "1.0",

  "description": "Allows for jk navigation of select pages.",
  "icons": {
    "48": "icons/icon-48.png"
  },

  "permissions": [
    "activeTab",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["jquery-3.3.1.min.js", "js/background-script.js"]  
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.3.1.min.js", "js/contentScript.js"],
      "run_at": "document_start"
    }
  ]



}
  
