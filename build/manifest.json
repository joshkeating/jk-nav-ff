{

  "manifest_version": 2,
  "name": "jk-nav-ff",
  "version": "1.0",

  "description": "this does something",
  "homepage_url": "",
  "icons": {
    "48": "icons/"
  },

  "permissions": [
    "activeTab"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.3.1.min.js", "js/contentScript.js"],
      "run_at": "document_start"
    }
  ],

  "background": {
    "scripts": ["jquery-3.3.1.min.js", "js/background-script.js"]  
  },

  "web_accessible_resources": [

  ]

}
  
